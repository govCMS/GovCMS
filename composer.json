{
    "name": "govcms/govcms",
    "description": "GovCMS8 Drupal Distribution",
    "keywords": ["GovCMS", "GovCMS8"],
    "type": "drupal-profile",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.govcms.gov.au",
    "support": {
        "issues:": "https://github.com/govCMS/govCMS8/issues",
        "wik": "https://github.com/govCMS/govCMS8/wiki",
        "source": "https://github.com/govCMS/govCMS8/releases"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {
            "type": "package",
            "package": {
                "type": "drupal-theme",
                "name": "govcms-custom/govcms8_uikit",
                "version": "v1.0",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/govCMS/govcms8_uikit/archive/1.x.zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "type": "drupal-theme",
                "name": "govcms-custom/govcms8_uikit_starter",
                "version": "v1.0",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/govCMS/govcms8_uikit_starter/archive/1.x.zip"
                }
            }
        }
    ],
    "require": {
        "php": "^7.1",
        "bower-asset/chosen": "1.8.7",
        "bower-asset/dropzone": "5.5.0",
        "cweagans/composer-patches": "^1.6",
        "drupal-composer/drupal-scaffold": "^2.4",
        "drupal/admin_toolbar": "1.24.0",
        "drupal/adminimal_admin_toolbar": "1.8.0",
        "drupal/adminimal_theme": "1.3",
        "drupal/chosen": "2.6.0",
        "drupal/components": "1.0.0",
        "drupal/config_filter": "1.3.0",
        "drupal/config_split": "1.4",
        "drupal/consumers": "1.2.0",
        "drupal/contact_storage": "1.0-beta9",
        "drupal/context": "4.0-beta2",
        "drupal/core": "8.6.2",
        "drupal/dropzonejs": "2.0.0-alpha3",
        "drupal/ds": "3.1",
        "drupal/entity_browser": "2.0",
        "drupal/entity_class_formatter": "1.1.0",
        "drupal/entity_embed": "1.0-beta2",
        "drupal/entity_reference_display": "1.2.0",
        "drupal/entity_reference_revisions": "1.6",
        "drupal/environment_indicator": "3.3",
        "drupal/facets": "1.1",
        "drupal/features": "3.8",
        "drupal/field_group": "3.0-beta1",
        "drupal/focal_point": "1.0-beta6",
        "drupal/google_analytics": "3.0-beta1",
        "drupal/govcms8_ui": "1.0.0-alpha1",
        "drupal/graphql": "3.0.0-rc2",
        "drupal/honeypot": "1.29",
        "drupal/inline_entity_form": "1.0-rc1",
        "drupal/jsonapi": "2.0-beta2",
        "drupal/linked_field": "1.1.0",
        "drupal/linkit": "5.0-beta7",
        "drupal/login_security": "1.5",
        "drupal/memcache": "2.0-rc2",
        "drupal/menu_trail_by_path": "1.1",
        "drupal/metatag": "1.7",
        "drupal/modifiers": "1.2",
        "drupal/module_filter": "3.1",
        "drupal/panelizer": "4.1",
        "drupal/panels": "4.3",
        "drupal/paragraphs": "1.3",
        "drupal/password_policy": "3.0-alpha4",
        "drupal/pathauto": "1.3",
        "drupal/permissions_by_term": "2.0",
        "drupal/recaptcha": "2.3",
        "drupal/redirect": "1.3",
        "drupal/restui": "1.15",
        "drupal/robotstxt": "1.2",
        "drupal/search_api": "1.10",
        "drupal/search_api_solr": "2.2",
        "drupal/seckit": "1.1",
        "drupal/shield": "1.2",
        "drupal/simple_oauth": "3.10",
        "drupal/simple_sitemap": "2.12",
        "drupal/swiftmailer": "1.0-beta2",
        "drupal/token": "1.5",
        "drupal/username_enumeration_prevention": "1.0-beta2",
        "drupal/video_embed_field": "2.0",
        "drupal/webform": "5.0.0-rc22",
        "govcms-custom/govcms8_uikit": "^1.0",
        "govcms-custom/govcms8_uikit_starter": "^1.0",
        "oomphinc/composer-installers-extender": "^1.1",
        "swiftmailer/swiftmailer": "5.4.12",
        "symfony/phpunit-bridge": "^3.4.6"
    },
    "require-dev": {
        "bex/behat-screenshot": "^1.2",
        "composer/composer": "^1.5",
        "dmore/behat-chrome-extension": "^1.1",
        "drupal/coder": "^8.3",
        "drupal/console": "^1.3",
        "drupal/devel": "^1.2",
        "drupal/drupal-extension": "^3.4",
        "drush/drush": "~9",
        "grasmash/drupal-security-warning": "^1.1",
        "jakoch/phantomjs-installer": "^2.1",
        "jcalderonzumba/mink-phantomjs-driver": "~0.3",
        "mikey179/vfsStream": "^1.6",
        "phing/phing": "^2.16",
        "phpunit/phpunit": "^4.8.35 || ^6.5",
        "se/selenium-server-standalone": "^3.6",
        "squizlabs/php_codesniffer": "^3.3"
    },
    "extra": {
        "drupal-scaffold": {
            "initial": {
                "sites/default/default.services.yml": "sites/default/services.yml",
                "sites/default/default.settings.php": "sites/default/settings.php"
            },
            "excludes": [
                "sites/development.services.yml"
            ]
        },
        "installer-types": ["bower-asset", "npm-asset"],
        "installer-paths": {
            "docroot/core": ["type:drupal-core"],
            "docroot/modules/contrib/{$name}": ["type:drupal-module"],
            "docroot/modules/custom/{$name}": ["type:drupal-custom-module"],
            "docroot/profiles/{$name}": ["type:drupal-profile"],
            "docroot/themes/contrib/{$name}": ["type:drupal-theme"],
            "docroot/themes/custom/{$name}": ["type:drupal-custom-theme"],
            "docroot/libraries/{$name}": ["type:drupal-library", "type:bower-asset", "type:npm-asset"],
            "drush/contrib/{$name}": ["type:drupal-drush"]
        },
        "patches": {}
    },
    "autoload": {
        "psr-4": {
            "govCMS\\Core\\": "src"
        }
    },
    "config": {
        "bin-dir": "bin/",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "conflict": {
        "drupal/core": "7.*",
        "drush/drush": "<8.1.10"
    },
    "scripts": {
        "post-install-cmd": [
            "PhantomInstaller\\Installer::installPhantomJS",
            "@composer drupal-scaffold",
            "@composer push"
        ],
        "post-update-cmd": [
            "PhantomInstaller\\Installer::installPhantomJS",
            "@composer push",
            "@composer package"
        ],
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "package": "govCMS\\Core\\Composer\\Package::execute",
        "push": "./bin/phing push",
        "nuke": "rm -r -f composer.lock bin docroot vendor"
    },
    "suggest": {
        "hirak/prestissimo": "^0.3"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
