        "drupal/entity_embed": "1.3.0",
        "drupal/entity_class_formatter": "2.0.0",
        "drupal/entity_hierarchy": "3.3.5",
        "drupal/entity_reference_display": "2.0.0",
        "drupal/entity_reference_revisions": "1.10.0",
        "drupal/environment_indicator": "4.0.14",
        "drupal/events_log_track": "3.1.5",
        "drupal/facets": "2.0.6",
        "drupal/field_group": "3.4.0",
        "drupal/focal_point": "1.5.0",
        "drupal/google_analytics": "4.0.2",
        "drupal/honeypot": "2.1.2",
        "drupal/govcms_dlm": "2.0.0",
        "drupal/inline_entity_form": "1.0.0-rc15",
