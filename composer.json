        "drupal/bigmenu": "2.0.0-rc3",
        "drupal/captcha": "1.10.0",
        "drupal/chosen": "4.0.0",
        "drupal/components": "2.4.0",
        "drupal/config_filter": "2.4.0",
        "drupal/config_ignore": "2.4.0",
        "drupal/config_perms": "2.0",
        "drupal/config_split": "1.5.0",
