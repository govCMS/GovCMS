        "drupal/contact_storage": "1.3.0",
        "drupal/context": "4.1.0",
        "drupal/core-composer-scaffold": "^9",
        "drupal/core-recommended": "9.4.14",
        "drupal/devel": "5.1.1",
        "drupal/crop": "2.3.0",
        "drupal/ctools": "3.13.0",
        "drupal/diff": "1.1.0",
        "drupal/dropzonejs": "2.8.0",
        "drupal/ds": "3.15.0",
        "drupal/dynamic_entity_reference": "1.16.0",
        "drupal/embed": "1.6.0",
        "drupal/encrypt": "3.1.0",
